/**app.wxss**/
Page {
  background: #eee;
}
image {
  width: 100%;
  height: 100%;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
/* 字体图标开始========= */
@font-face {
  font-family: 'iconfont';  /* project id 1450175 */
  src: url('//at.alicdn.com/t/font_1450175_pt84gejxnk.eot');
  src: url('//at.alicdn.com/t/font_1450175_pt84gejxnk.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1450175_pt84gejxnk.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1450175_pt84gejxnk.woff') format('woff'),
  url('//at.alicdn.com/t/font_1450175_pt84gejxnk.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1450175_pt84gejxnk.svg#iconfont') format('svg');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-paperfile:before {
  content: "\e621";
}

.icon-gou_wu_che2:before {
  content: "\e619";
}

.icon-wo_de2:before {
  content: "\e61d";
}

.icon-xinxi:before {
  content: "\e642";
}
/* 字体图标结束=========== */


